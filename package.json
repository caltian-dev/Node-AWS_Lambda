{
  "name": "@baselime/tracer-node",
  "version": "1.0.7",
  "description": "",
  "main": "build/index.js",
  "files": [
    "/lambda-wrapper.js",
    "/build/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run package:tracer && npm run package:lib && npm run package:typedefs",
    "package:typedefs": "npx tsc lib/index.ts --emitDeclarationOnly --declaration --outDir build",
    "package:lib": "esbuild lib/index.ts --bundle --minify --platform=node --target=node16 --outfile=build/index.js",
    "package:tracer": "esbuild src/tracer.ts --bundle --minify --platform=node --target=node16 --outfile=lambda-wrapper.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Baselime/tracer-node.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Baselime/tracer-node/issues"
  },
  "homepage": "https://github.com/Baselime/tracer-node#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "esbuild": "^0.16.16",
    "@opentelemetry/exporter-trace-otlp-http": "^0.34.0",
    "@opentelemetry/instrumentation": "^0.34.0",
    "@opentelemetry/instrumentation-aws-lambda": "^0.34.0",
    "@opentelemetry/instrumentation-aws-sdk": "^0.33.0",
    "@opentelemetry/instrumentation-http": "^0.34.0",
    "@opentelemetry/sdk-trace-base": "^1.8.0",
    "@opentelemetry/sdk-trace-node": "^1.8.0",
    "typescript": "^4.9.4"
